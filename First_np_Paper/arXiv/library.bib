journal = {International Journal of Radiation Biology},
number = {0},
pages = {1--9},
title = {{Triple targeting of Auger emitters using octreotate conjugated to a DNA-binding ligand and a nuclear localizing signal}},
url = {http://www.tandfonline.com/doi/full/10.3109/09553002.2016.1157278},
