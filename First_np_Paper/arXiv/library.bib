@article{Hansen1974,
abstract = {The decay of115mIn has been investigated using accurate counting methods. The emission rate of conversion electrons plus$\beta$−-particles was determined with a 4$\pi$ proportional flow counter. The total andK-shell internal conversion coefficients of the 336 keV$\gamma$-ray in115In were measured by the electron X-ray coincidence method using combinations of a Si surface barrier with a NaI(Tl) detector and of a magnetic$\beta$-spectrometer with a high energy resolution Si(Li) detector, respectively. The conversion ratioR=K/(L+M+...) was deduced from electron spectra recorded with the magnetic$\beta$-spectrometer. The 336 keV$\gamma$-ray emission rate of all used sources was determined with a calibrated NaI(Tl)$\gamma$-ray spectrometer. A Ge(Li) detector has been used to determine the relative intensity of the 497 keV$\gamma$-ray in115Sn. As results have been deduced the 336 keV$\gamma$-ray emission per decay (N$\gamma$1/N0=(45.9 {\textpm} 0.1){\%}), the total internal conversion coefficient ($\alpha$=1.073 {\textpm} 0.014), theK-shell internal conversion coefficient ($\alpha$K=0.843{\textpm}0.012), the conversion ratioR=3.63{\textpm}0.07, the$\beta$−-transition per decay going to the ground state (N$\beta$1/N0=(5.0 {\textpm} 0.7){\%}) and to the first excited level in115Sn115Sn(N$\beta$2/N0=(0.047 {\textpm} 0.002){\%}), and the 497 keV$\gamma$-ray emission (N$\gamma$2/N$\gamma$1=(0.103 {\textpm} 0.004){\%}). From the obtained internal conversion data it follows that the 336 keV$\gamma$-ray transition is ofM4 character with anE5 admixture of less than (3.5{\textpm}1.5){\%}. The half-life of the isomeric state115mIn has been determined with four different methods. The result isT1/2=(4.486{\textpm}0.004) h.},
author = {Hansen, H H and de Roost, E and van der Eijk, W and Vaninbroukx, R},
doi = {10.1007/BF01669057},
file = {:media/andrew/Storage/Mendeley/Hansen et al. - 1974 - The decay of 115mIn.pdf:pdf},
