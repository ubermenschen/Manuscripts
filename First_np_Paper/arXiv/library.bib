@article{Grogan201362,
abstract = {Abstract The detonation of a terrorist nuclear weapon in the United States would result in the massive loss of life and grave economic damage. Even if a device was not detonated, its known or suspected presence aboard a cargo container ship in a U.S. port would have major economic and political consequences. One possible means to prevent this threat would be to board a ship at sea and search for the device before it reaches port. The scenario considered here involves a small Coast Guard team with strong intelligence boarding a container ship to search for a nuclear device. Using active interrogation, the team would nonintrusively search a block of shipping containers to locate the fissile material. Potential interrogation source and detector technologies for the team are discussed. The methodology of the scan is presented along with a technique for calculating the required interrogation source strength using computer simulations. {MCNPX} was used to construct a computer model of a container ship, and several search scenarios were simulated. The results of the simulations are presented in terms of the source strength required for each interrogation scenario. Validation measurements were performed in order to scale these simulation results to expected performance. Interrogations through the short (2.4&#xa0;m) axis of a standardized shipping container appear to be feasible given the entire range of container loadings tested. Interrogations through several containers at once or a single container through its long (12.2&#xa0;m) axis do not appear to be viable with a portable interrogation system. },
author = {Grogan, Brandon R and Henkel, James J and Johnson, Jeffrey O and Mihalczo, John T and Miller, Thomas M and Patton, Bruce W},
doi = {http://dx.doi.org/10.1016/j.nimb.2013.08.037},
issn = {0168-583X},
journal = {Nuclear Instruments and Methods in Physics Research Section B: Beam Interactions with Materials and Atoms},
keywords = {Active interrogation,Boarded search,Cargo container,Monte Carlo,Special nuclear material},
number = {0},
pages = {62--70},
title = {{Investigation of active interrogation techniques to detect special nuclear material in maritime environments: Boarded search of a cargo container ship}},
url = {http://www.sciencedirect.com/science/article/pii/S0168583X1300921X},
volume = {316},
year = {2013}
}
